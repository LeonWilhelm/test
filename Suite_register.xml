		<listener class-name="com.eloancn.shared.common.TestngListener" />
		<listener class-name="com.eloancn.shared.common.RetryListener"/>
	</listeners>
	 <test name="debit" >
		<classes>
			<class name="com.eloancn.suites.debit.Debit"/>
		</classes>
	</test>
	<test name="credit" >
